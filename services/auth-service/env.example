JWT_RF_EXPIRES_IN=7d

# RabbitMQ Configuration
RABBITMQ_URL=amqp://guest:guest@ibanking_rabbitmq:5672

# Redis Configuration (if needed for sessions)
REDIS_URL=redis://ibanking_redis:6379